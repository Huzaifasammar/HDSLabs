<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/homeFragmentMain">

    <fragment
        android:id="@+id/aboutFragmentMain"
        android:name="com.hds.labs.ids.hdslabs.fragments.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />

    <fragment
        android:id="@+id/homeFragmentMain"
        android:name="com.hds.labs.ids.hdslabs.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragmentMain_to_locationFragmentMain"
            app:destination="@id/locationFragmentMain" />
    </fragment>

    <fragment
        android:id="@+id/locationFragmentMain"
        android:name="com.hds.labs.ids.hdslabs.fragments.LocationFragment"
        android:label="fragment_location"
        tools:layout="@layout/fragment_location" />
    <fragment
        android:id="@+id/accountFragment2"
        android:name="com.hds.labs.ids.hdslabs.fragments.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />

</navigation>